#!/bin/bash

N=10  # начальное значение параметра N
N1=20  # конечное значение параметра N
step=2  # шаг увеличения второго параметра
param1=1  # начальное значение первого параметра

while [ $param1 -le 4 ]
do
    param2=$N  # начальное значение второго параметра
    while [ $param2 -le $N1 ]
    do
        ./ваша_программа $param1 $param2 >> результаты.txt  # запуск вашей программы с параметрами и запись результата в файл
        ((param2 += step))  # увеличение второго параметра на заданный шаг
    done
    ((param1++))  # увеличение первого параметра на 1
done
